<% var title = 'Support' %>
<% include head %>

<h1>Support form</h1>
<%- message %>
<form method="post" action="/support">
  <p>
    <label>Input your message to admins:</label></p>
    <textarea width=350 height=120 name="message"></textarea>
  <p>
    <label>Email (optional):</label>
     </p>
    <input type="text" name="email">
 
  <p>
    <input type="submit" value="Send message">
  </p>
</form>

<% include foot %>
